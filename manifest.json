{
  "name": "IPry",
  "description": "Gives your current IP address whenever you click it!",
  "version": "1.0",
  "manifest_version": 2,

  "browser_action": {
      "default_popup": "popup.html",
      "default_icon" : "logo.png"
  },
  "icons" : {
      "100": "logo.png"
      
  },
  "permissions" : ["activeTab"]
}
